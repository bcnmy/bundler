{
  "openrpc": "1.2.4",
  "info": {
    "title": "Biconomy Bundler",
    "version": "V3 (Entrypoint v0.7.0)",
    "description": "A scalable, hosted [ERC-4337](https://eips.ethereum.org/EIPS/eip-4337) compatible Bundler make by [Biconomy](https://biconomy.io)."
  },
  "servers": [
    {
      "name": "Production",
      "url": "https://bundler.biconomy.io/api/v3/<chainId>/<apiKey>"
    }
  ],
  "methods": [
    {
      "name": "eth_chainId",
      "description": "Returns a chain id, a value used in replay-protected transaction signing as introduced by EIP-155, if supported by the bundler. Error otherwise. See the full list at https://docs.biconomy.io/supportedNetworks",
      "summary": "Return a chain ID, if supported",
      "params": [
        {
          "name": "id",
          "description": "Chain ID in decimal",
          "summary": "chain ID",
          "required": true,
          "schema": {
            "type": "number"
          }
        }
      ],
      "result": {
        "name": "chainId",
        "description": "hex format integer of the current chain id",
        "schema": {
          "title": "chainId",
          "type": "string",
          "pattern": "^0x[a-fA-F\\d]+$"
        }
      },
      "examples": [
        {
          "name": "Base Mainnet",
          "params": [],
          "result": {
            "name": "Base Mainnet chain ID in hex",
            "value": "0x2105"
          }
        }
      ]
    },
    {
      "name": "eth_estimateUserOperationGas",
      "summary": "Estimate the gas values for a UserOperation",
      "description": "Given UserOperation with default gas limits and gas prices, return the estimated needed gas limits required to submit the User Operation to the EntryPoint contract.",
      "params": [
        {
          "name": "PartialUserOperation",
          "required": true,
          "schema": {
            "$ref": "#/components/schemas/PartialUserOperation"
          }
        },
        {
          "name": "EntryPointAddress",
          "required": true,
          "summary": "EPv0.7.0: 0x0000000071727De22E5E9d8BAf0edAc6f37da032",
          "schema": {
            "$ref": "#/components/schemas/Address"
          }
        },
        {
          "name": "State Override Set",
          "schema": {
            "$ref": "#/components/schemas/StateOverrideSet"
          }
        }
      ],
      "result": {
        "name": "Entrypoint Gas Estimates",
        "description": "Estimated gas values to submit a UserOp to the Entrypoint contract",
        "schema": {
          "$ref": "#/components/schemas/GasEstimates"
        }
      },
      "examples": [
        {
          "name": "Basic Example",
          "params": [
            {
              "name": "Partial User Op",
              "value": {
                "sender": "0x0a972b2e3A1B3C2EEf8d49F33051668D35CEC345",
                "maxFeePerGas": "100000",
                "maxPriorityFeePerGas": "1000000",
                "callData": "",
                "factory": "0x00000bb19a3579F4D779215dEf97AFbd0e30DB55",
                "factoryData": "",
                "nonce": "97069576985704150461231402382789097232549411334249332225275362800021683568640",
                "signature": "0x"
              }
            },
            {
              "name": "Entrypoint Contract Address",
              "value": "0x0000000071727De22E5E9d8BAf0edAc6f37da032"
            }
          ],
          "result": {
            "name": "User Operation",
            "value": {
              "callGasLimit": 563623,
              "verificationGasLimit": 290241,
              "preVerificationGas": 287109,
              "paymasterPostOpGasLimit": 290241,
              "paymasterVerificationGasLimit": 290241,
              "maxPriorityFeePerGas": 1000000,
              "maxFeePerGas": 1001592
            }
          }
        }
      ]
    },
    {
      "name": "eth_sendUserOperation",
      "description": "Submit a User Operation object to the User Operation pool of the bundler. The result userOpHash is returned if and only if the request passed simulation and was accepted in the clientâ€™s User Operation pool. If the validation, simulation, or User Operation pool inclusion fails, a failure reason is returned",
      "summary": "Submit a User Operation object to the User Operation pool of the bundler",
      "params": [
        {
          "name": "UserOperation",
          "description": "A full user-operation struct. All fields MUST be set as hex values. Empty bytes block (e.g. empty callData) MUST be set to 0x",
          "summary": "A full user-operation struct",
          "required": true,
          "schema": {
            "$ref": "#/components/schemas/UserOperation"
          }
        }
      ],
      "result": {
        "name": "userOpHash",
        "description": "The hash of the User Operation",
        "schema": {
          "$ref": "#/components/schemas/UserOpHash"
        }
      }
    },
    {
      "name": "eth_getUserOperationByHash",
      "summary": "Return a UserOperation based on a userOpHash",
      "description": "Return a UserOperation based on a hash (userOpHash) returned by eth_sendUserOperation",
